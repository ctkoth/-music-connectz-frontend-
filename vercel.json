{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/config.js", "dest": "/api/config.js" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|woff|woff2|ttf|eot))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
